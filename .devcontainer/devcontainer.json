{
  "name": "Ultra.js",
  "build": {
    "dockerfile": "Dockerfile",
    // Update 'VARIANT' to pick an Debian OS version: bullseye, buster
    "args": {
      "VARIANT": "bullseye"
    }
  },
  // This will install the vscode-deno extension
  "extensions": [
    "denoland.vscode-deno",
    "eamodio.gitlens"
  ],
  "remoteUser": "vscode"
}
